all:
	$(MAKE) -C homer_lib/Release/	all
	$(MAKE) -C homer_app/Release/	all
	$(MAKE) -C homer_lib/Debug/	all
	$(MAKE) -C homer_app/Debug/	all	
debug:
	$(MAKE) -C homer_lib/Debug/	all
	$(MAKE) -C homer_app/Debug/	all	
release:	
	$(MAKE) -C homer_lib/Release/	all
	$(MAKE) -C homer_app/Release/	all
clean:
	$(MAKE) -C homer_lib/Release/	clean
	$(MAKE) -C homer_app/Release/	clean
	$(MAKE) -C homer_lib/Debug/	clean
	$(MAKE) -C homer_app/Debug/	clean
